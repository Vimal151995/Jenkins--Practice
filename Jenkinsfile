pipeline {
    agent any
    
    stages{
        stage ("Build") {
            steps {
                echo "This is build stage"
                echo "Code is Building"
            }
        }
        stage ("Test") {
            steps {
                echo "This is test stage"
                echo "Testing in progress"
                echo "Testing completed"
            }
        }
        stage ("Deploy") {
            steps {
                echo "This is deploy stage"
                echo "Code is Deployed"
            }
        }
    }
}
